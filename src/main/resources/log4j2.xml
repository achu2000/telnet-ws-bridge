<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="error" packages="cn.easyplay.proxy" monitorInterval="1800">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS}|%F|%L|%msg%n%ex{short.message}" />
		</Console>
	</Appenders>
	<Loggers>
		<logger name="cn.easyplay.proxy" level="debug" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<root level="info">
			<appender-ref ref="Console" />
		</root>
	</Loggers>
</Configuration>  